{
  "name": "dama",
  "version": "0.0.0",
  "description": "Dama",
  "main": "main.js",
  "license": "MIT",
  "scripts": {
    "start": "electron .",
    "build": "cross-env NODE_ENV=production browserify ./app/app.js -t babelify -g uglifyify | uglifyjs --compress --mangle > ./public/js/generated/bundle.js",
    "watch": "cross-env NODE_ENV=development watchify ./app/app.js -t babelify -o ./public/js/generated/bundle.js --debug --verbose",
    "test": "standard"
  },
  "author": {
    "name": "Peter Koscelansky"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/koscelansky/Dama.git"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-styled-components": "^1.5.1",
    "babel-plugin-transform-inline-environment-variables": "^0.4.0",
    "babel-plugin-transform-react-jsx-source": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babelify": "^8.0.0",
    "cross-env": "^5.1.4",
    "electron": "^1.8.4",
    "electron-reload": "^1.2.2",
    "standard": "^11.0.0",
    "uglify-js": "^3.3.16",
    "uglifyify": "^4.0.5",
    "watchify": "^3.11.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "lodash": "^4.17.5",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-dnd": "^2.5.4",
    "react-dnd-touch-backend": "^0.3.21",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "reselect": "^3.0.1",
    "styled-components": "^3.2.0"
  }
}
