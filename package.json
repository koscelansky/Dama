{
  "name": "dama",
  "version": "0.0.0",
  "description": "Dama",
  "private": true,
  "main": "main.js",
  "license": "MIT",
  "scripts": {
    "start": "electron .",
    "build": "cross-env NODE_ENV=production browserify ./app/app.js -p [ tinyify --no-flat ] -t babelify -o ./public/js/generated/bundle.js",
    "watch": "cross-env NODE_ENV=development watchify ./app/app.js -t babelify -o ./public/js/generated/bundle.js --debug --verbose",
    "test": "standard && stylelint ./app/**/**.js"
  },
  "author": {
    "name": "Peter Koscelansky"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/koscelansky/Dama.git"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-styled-components": "^1.5.1",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "babel-plugin-transform-react-jsx-source": "^6.22.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babelify": "^8.0.0",
    "cross-env": "^5.2.0",
    "electron": "^2.0.3",
    "electron-reload": "^1.2.2",
    "standard": "^11.0.0",
    "stylelint": "9.3.0",
    "stylelint-config-recommended": "^2.1.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.3.1",
    "tinyify": "^2.4.3",
    "watchify": "^3.11.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "immutable": "^3.8.2",
    "lodash": "^4.17.5",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-dnd": "^5.0.0",
    "react-dnd-touch-backend": "^0.4.0",
    "react-dom": "^16.4.1",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "reselect": "^3.0.1",
    "styled-components": "^3.3.3",
    "webworkify": "^1.5.0"
  }
}
